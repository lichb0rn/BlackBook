filters:
  and:
    - file.tags.contains("#type/project")
    - file.tags.containsAny("#status/active", "#status/paused")
    - '!file.folder.startsWith("x")'
properties:
  note.summary:
    displayName: Next Action
views:
  - type: table
    name: Table
    order:
      - file.name
      - up
      - summary
    columnSize:
      file.name: 367
      note.up: 407
