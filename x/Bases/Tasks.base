views:
  - type: table
    name: Table
    filters:
      and:
        - file.folder == "10_Tasks"
    order:
      - file.name
      - summary
      - created
    sort:
      - property: file.mtime
        direction: DESC
      - property: file.name
        direction: ASC
    columnSize:
      file.name: 657
      note.summary: 312
