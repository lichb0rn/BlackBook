views:
  - type: table
    name: Table
    filters:
      and:
        - file.folder == "10_Tasks"
    order:
      - file.name
      - created
      - file.mtime
    sort:
      - property: file.mtime
        direction: DESC
      - property: file.name
        direction: ASC
    columnSize:
      file.name: 783
